        <owl:inverseOf rdf:resource="http://www.loa-cnr.it/ontologies/DUL.owl#isRoleDefinedIn"/>
        <rdfs:range rdf:resource="http://www.loa-cnr.it/ontologies/DUL.owl#Role"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relation between a description and a role, e.g. the recipe for a cake defines the role &apos;ingredient&apos;.</rdfs:comment>
        <rdfs:label xml:lang="en">defines role</rdfs:label>
        <rdfs:label xml:lang="it">definisce il ruolo</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.loa-cnr.it/ontologies/DUL.owl#definesTask -->

    <owl:ObjectProperty rdf:about="http://www.loa-cnr.it/ontologies/DUL.owl#definesTask">
        <rdfs:subPropertyOf rdf:resource="http://www.loa-cnr.it/ontologies/DUL.owl#defines"/>
        <owl:inverseOf rdf:resource="http://www.loa-cnr.it/ontologies/DUL.owl#isTaskDefinedIn"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relation between a description and a task, e.g. the recipe for a cake defines the task &apos;boil&apos;.</rdfs:comment>
        <rdfs:label xml:lang="en">defines task</rdfs:label>
        <rdfs:label xml:lang="it">definisce il task</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.loa-cnr.it/ontologies/DUL.owl#describes -->

    <owl:ObjectProperty rdf:about="http://www.loa-cnr.it/ontologies/DUL.owl#describes">
