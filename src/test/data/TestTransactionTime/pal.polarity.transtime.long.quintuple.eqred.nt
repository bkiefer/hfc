#
# a collection of special XSD data types for the PAL diabetes domain
#
# NOTE: this file should be loaded in PAL always together with file
#         default.polarity.transtime.long.quintuple.eqred.nt
#       which contains the domain-independent axiomatic quintuples
#       for transaction time
#
# @author Hans-Ulrich Krieger
# @version Tue Jun  7 11:10:33 CEST 2016
#


<logic:true> <xsd:min-1> <rdf:type> <rdfs:Datatype> "0"^^<xsd:long> .   # = 1/min
<logic:true> <xsd:cal> <rdf:type> <rdfs:Datatype> "0"^^<xsd:long> .     # = calorie
<logic:true> <xsd:cm> <rdf:type> <rdfs:Datatype> "0"^^<xsd:long> .      # = centimeter
<logic:true> <xsd:gm> <rdf:type> <rdfs:Datatype> "0"^^<xsd:long> .      # = gram
<logic:true> <xsd:iu> <rdf:type> <rdfs:Datatype> "0"^^<xsd:long> .      # = international unit
<logic:true> <xsd:kg> <rdf:type> <rdfs:Datatype> "0"^^<xsd:long> .      # = kilogram
<logic:true> <xsd:kg_m2> <rdf:type> <rdfs:Datatype> "0"^^<xsd:long> .   # = body mass index
<logic:true> <xsd:m> <rdf:type> <rdfs:Datatype> "0"^^<xsd:long> .       # = meter
<logic:true> <xsd:mg_dL> <rdf:type> <rdfs:Datatype> "0"^^<xsd:long> .   # = blood glucose (US)
<logic:true> <xsd:mmHg> <rdf:type> <rdfs:Datatype> "0"^^<xsd:long> .    # = blood preasure
<logic:true> <xsd:mmol_L> <rdf:type> <rdfs:Datatype> "0"^^<xsd:long> .  # = blood glucose (EU etc.)
